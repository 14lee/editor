{
  "name": "@umoteam/editor",
  "version": "2.0.0-beta.1",
  "description": "Umo Editor is an open-source document editor based on Vue3. It offers comprehensive document editing capabilities, supports Markdown syntax, and basic rich-text editing functions. It also supports the insertion of various formatted node types, provides a variety of practical tools, and allows for page style customization. Additionally, it supports exporting in multiple formats, printing and print preview, document block editing, the addition of custom extensions, and multi-language settings.",
  "keywords": [
    "umo editor",
    "umodoc editor",
    "document editor",
    "docs editor",
    "word editor",
    "office edtior",
    "web editor",
    "richtext editor",
    "wysiwyg editor",
    "vue editor",
    "vue3 editor",
    "editor",
    "web office",
    "online office"
  ],
  "author": {
    "name": "umodoc",
    "email": "contact@umodoc.com",
    "url": "https://umodoc.com"
  },
  "license": "MIT",
  "homepage": "https://umodoc.com",
  "bugs": {
    "url": "https://github.com/umodoc/editor/issues",
    "email": "support@umodoc.com"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/umo-editor.mjs"
    }
  },
  "scripts": {
